{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "cursorOffset": -1,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": true,
  "jsxSingleQuote": true,
  "proseWrap": "always",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "useTabs": true
}
